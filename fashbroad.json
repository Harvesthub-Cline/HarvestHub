  "courseDetails": {
    "courseId": "course_001",
    "title": "Container Gardening Basics",
    "description": "A comprehensive guide to growing food in containers, perfect for urban dwellers with limited space.",
    "detailedDescription": "This course will transform how you think about gardening in small spaces. You'll learn proven techniques for selecting containers, preparing soil, choosing plants, and maintaining healthy container gardens year-round.",
    
    "instructor": {
      "name": "Maria Rodriguez",
      "bio": "With over 15 years of experience in urban agriculture, Maria has helped thousands of city dwellers grow their own food. She holds a Master's in Sustainable Agriculture and has consulted for urban farming projects worldwide.",
      "credentials": [
        "MS Sustainable Agriculture, Cornell University",
        "Certified Permaculture Designer",
        "Urban Agriculture Consultant for UN Food Programs"
      ],
      "socialLinks": {
        "website": "https://mariasgarden.com",
        "twitter": "@mariasgarden",
        "instagram": "marias_urban_garden"
      }
    },
    
